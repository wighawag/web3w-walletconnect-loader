{
  "name": "web3w-walletconnect-loader",
  "version": "0.3.0",
  "description": "Wallet Connect module loader for web3w",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "author": "Ronan Sandford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh@github.com:wighawag/web3w-walletconnect-loader.git"
  },
  "keywords": [
    "web3w",
    "web3",
    "wallet",
    "walletconnect",
    "ethereum"
  ],
  "devDependencies": {
    "@ethersproject/bignumber": "^5.1.1",
    "@ethersproject/contracts": "^5.1.1",
    "@ethersproject/providers": "^5.1.2",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "terser": "^5.7.0",
    "terser-folder": "^2.0.0",
    "typescript": "^4.2.4",
    "web3w": "^0.3.0"
  },
  "scripts": {
    "prepare": "node ./.setup.js && npm run lint && npm run build",
    "dev": "tsc -w -p tsconfig.json",
    "build:esm": "rimraf dist/esm && tsc -p tsconfig.json",
    "build:cjs": "rimraf dist/cjs && tsc -p tsconfig-cjs.json",
    "build": "run-s build:esm build:cjs",
    "lint": "eslint src/**/*.{js,ts,tsx} --quiet --fix"
  },
  "dependencies": {
    "named-logs": "0.0.13"
  }
}
